c := plot_createCanvas();
g1 := plot_addGraph(c, "sin(x)", "simple sin");
g2 := plot_addListGraph(c, [[-10, 10], [-5, 0], [5, 3], [10, 10]], "name");
g3 := plot_addParamGraph(c, "x", "x+1", "name", [-5,5]);
l1 := plot_addLabel(c, [4,-4], "NameofLabel");
b1 := plot_addBullets(c, [[-8, 10]]);
plot_removeGraph(c, g3);
plot_removeGraph(c, g2);
plot_removeGraph(c, g1);
plot_removeGraph(c, l1);
plot_removeGraph(c, b1);
get("Press <Enter> to continue");
